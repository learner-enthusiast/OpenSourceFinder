{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: FastAPI (Uvicorn)",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.main:app", // path to your FastAPI app object
        "--reload"
      ],
      "jinja": false,
      "env": {
        "ENVIRONMENT": "development"
      },
      "console": "integratedTerminal"
    }
  ]
}
